{
  "name": "ifroautoriza-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://ifro-autoriza.rodighiero.com.br",
  "scripts": {
    "dev": "npm-run-all --parallel dev:vite tailwind:watch",
    "dev:vite": "vite",
    "tailwind:watch": "npx @tailwindcss/cli -i ./src/input.css -o ./src/output.css --watch",
    "build": "npx @tailwindcss/cli -i ./src/input.css -o ./src/output.css && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "@tailwindcss/cli": "^4.1.11",
    "@tanstack/react-query": "^5.85.0",
    "axios": "^1.11.0",
    "formik": "^2.4.6",
    "jwt-decode": "^4.0.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hot-toast": "^2.5.2",
    "react-router-dom": "^7.8.0",
    "tailwindcss": "^4.1.11",
    "yup": "^1.7.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "gh-pages": "^6.1.1",
    "globals": "^16.3.0",
    "npm-run-all": "^4.1.5",
    "vite": "^7.1.2"
  }
}
